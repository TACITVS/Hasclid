-- Test file for <= and < operators
-- Date: 2025-12-09

:verbose

-- Test 1: Simple <= with concrete values
:reset
(<= 3 5)

-- Test 2: Simple < with concrete values
:reset
(< 2 4)

-- Test 3: <= with expressions
:reset
(<= (+ x 1) (+ x 2))

-- Test 4: < with expressions
:reset
(< (^ x 2) (+ (^ x 2) 1))

-- Test 5: Circle constraint with <=
:reset
:point P x y
:point O 0 0
(<= (dist2 P O) 25)

-- Test 6: Inequality chain with <
:reset
(< (+ (^ x 2) (^ y 2)) 10)

-- Test 7: Mixed operators (>= and <=)
:reset
(and (>= x 0) (<= x 10))

-- Test 8: All four comparison operators
:reset
:point A x1 y1
:point B x2 y2
(and (>= (dist2 A B) 1) (<= (dist2 A B) 100))
