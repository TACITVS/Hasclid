-- Test rational elimination with explicit CAD

:verbose

-- TEST 1: Simple inequality with division
:assume (> x 0)
:assume (> y 0)
:cad (>= (/ x y) 0)

-- TEST 2: Division equals zero
:clear
:assume (= x 0)
:assume (> y 1)
:cad (= (/ x y) 0)

-- TEST 3: Morley-like expression
:clear
:assume (> sA2 0)
:assume (> tB 0)
:assume (= tA (/ 1 (+ sA2 (/ 1 tB))))
:cad (> tA 0)

-- TEST 4: Division with sqrt (combined elimination)
:clear
:assume (> n 0)
:cad (= (/ n (sqrt n)) (sqrt n))

-- TEST 5: Simple x/x = 1
:clear
:assume (> x 1)
:cad (= (/ x x) 1)
